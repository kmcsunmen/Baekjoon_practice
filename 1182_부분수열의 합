#include <stdio.h>

int cnt = 0;
int N, target;
int arr[20];

void dfs(int idx, int depth, int sum) {
	if (idx == N) {
		if (sum == target && depth != 0) cnt++;
		return;
	}

	//비선택
	dfs(idx + 1, depth, sum);
	//선택
	dfs(idx + 1, depth + 1, sum + arr[idx]);
}

int main() {
	scanf("%d %d", &N, &target);
	for (int i = 0; i < N; i++) {
		scanf("%d", arr + i);
	}

	dfs(0,0,0);

	printf("%d\n", cnt);
	return 0;
}
