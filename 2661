#include <stdio.h>
#include <stdlib.h> 

int N;
int the_num[81];

int isGood(int depth) {
	for (int len = 1; len*2 <= depth+1; len++) {
		int same = 1;
		for (int i = 0; i < len; i++) {
			if (the_num[depth - i] != the_num[depth - len - i]) {
				same = 0;
				break;
			}
		}
		if (same) return 0;
	}
	return 1;
}

void dfs(int depth) {
	if (depth == N) {
		for (int i = 0; i < N; i++) {
			printf("%d", the_num[i]);
		}
		printf("\n");
		exit(0);
	}

	for (int i = 1; i <= 3; i++) {
		the_num[depth] = i;
		if (isGood(depth)) dfs(depth + 1);
	}
}

int main() {
	scanf("%d", &N);

	dfs(0);

    return 0;
}
