#include <stdio.h>

int N, M;
int chosen[10];
int visited[10];

void dfs(int v, int depth) {
	visited[v] = 1;
	chosen[depth++] = v;

	if (depth == M) {
		for (int i = 0; i < M; i++) {
			printf("%d ", chosen[i]);
		}
		printf("\n");
		return;
	}

	for (int i = 1; i <= N; i++) {
		if (!visited[i]) {
			dfs(i, depth);
			visited[i] = 0;
		}
	}
}

int main() {
	scanf("%d %d", &N, &M);
	
	for (int n = 1; n <= N; n++) {
		for (int i = 1; i <= N; i++) {
			visited[i] = 0;
		}
		dfs(n, 0);
	}
	return 0;
}
